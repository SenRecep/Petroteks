@model (AboutUsObject Ins, string Value, int Power)

<li>

    <div class="media">
        <a asp-area="" asp-controller="Home" asp-action="AboutUs">
            <a href="#" class="pull-left">
                <i class="circle icofont-street-view"></i>
            </a>
            <div class="media-body">
                <span class="media-meta pull-right">@Model.Power Eşleşme</span>
